{
	"name": "@spon/ui",
	"version": "0.0.1",
	"private": true,
	"license": "MIT",
	"type": "module",
	"types": "./dist/index.d.ts",
	"main": "./dist/index.js",
	"exports": {
		"./*": [
			"./src/*/index.ts",
			"./src/*.ts",
			"./src/*/.css"
		],
		"./preset": {
			"source": "./src/preset.ts",
			"types": "./dist/preset.d.ts",
			"import": {
				"types": "./dist/preset.d.mts",
				"default": "./dist/preset.js"
			},
			"require": "./dist/preset.js"
		}
	},
	"scripts": {
		"lint": "eslint . --max-warnings 0 --fix",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf *.tsbuildinfo node_modules",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build",
		"build": "tsup && panda codegen",
		"panda": "panda codegen",
		"build-fast": "tsup --no-dts",
		"dev": "npm run build-fast --watch; npm run storybook"
	},
	"devDependencies": {
		"@chromatic-com/storybook": "^2.0.2",
		"@spon/eslint-config": "*",
		"@spon/typescript-config": "*",
		"@storybook/addon-essentials": "^8.3.2",
		"@storybook/addon-interactions": "^8.3.2",
		"@storybook/addon-links": "^8.3.2",
		"@storybook/addon-onboarding": "^8.3.2",
		"@storybook/addon-webpack5-compiler-swc": "^1.0.5",
		"@storybook/blocks": "^8.3.2",
		"@storybook/nextjs": "^8.3.2",
		"@storybook/react": "^8.3.2",
		"@storybook/react-webpack5": "^8.4.6",
		"@svgr/webpack": "^8.1.0",
		"@testing-library/react": "^16.0.1",
		"@types/node": "^22.5.5",
		"@types/react": "^18.3.0",
		"@types/react-dom": "^18.3.0",
		"eslint-plugin-storybook": "^0.8.0",
		"react": "18.3.1",
		"react-dom": "18.3.1",
		"storybook": "^8.3.2",
		"typescript": "^5.6.2"
	},
	"dependencies": {
		"@capsizecss/core": "^4.1.2",
		"@pandacss/dev": "^0.46.1",
		"@radix-ui/react-accordion": "^1.2.0",
		"@radix-ui/react-alert-dialog": "^1.1.1",
		"@radix-ui/react-collapsible": "^1.1.0",
		"@radix-ui/react-dialog": "^1.1.1",
		"@radix-ui/react-dismissable-layer": "^1.1.0",
		"@radix-ui/react-dropdown-menu": "^2.1.1",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-label": "^2.1.0",
		"@radix-ui/react-radio-group": "^1.2.0",
		"@radix-ui/react-select": "^2.1.1",
		"@radix-ui/react-separator": "^1.1.0",
		"@radix-ui/react-slot": "^1.1.0",
		"@radix-ui/react-tabs": "^1.1.0",
		"@radix-ui/react-toast": "^1.2.1",
		"@radix-ui/react-toggle": "^1.1.0",
		"@radix-ui/react-toggle-group": "^1.1.0",
		"@spon/styled-system": "*",
		"@vitejs/plugin-react": "^4.3.1",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"cmdk": "^1.0.0",
		"embla-carousel-react": "^8.3.0",
		"framer-motion": "^11.5.6",
		"react-remove-scroll": "^2.6.0",
		"usehooks-ts": "^3.1.0"
	}
}
